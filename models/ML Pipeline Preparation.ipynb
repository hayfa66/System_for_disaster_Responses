    "        ) \n",
    "\n",
    "ct = ColumnTransformer( [('CatTr',pipeline,0)] , remainder='passthrough') \n",
    "\n",
    "Newpi = Pipeline([ ('cr', ct) ,  ('MltRFC',\n",
    "                MultiOutputClassifier(RandomForestClassifier())) ])\n",
    "\n",
    "Newpi.fit(X_train, y_train)\n",
    "\n",
    "y_pred = Newpi.predict(X_test)\n",
